# Example Kubernetes secret for OAuth2-Proxy Admin Panel
# Copy this file to secret.yaml and replace the values with your actual secrets
# DO NOT commit secret.yaml to version control!

apiVersion: v1
kind: Secret
metadata:
  name: oauth2-proxy-admin
  namespace: librechat
type: Opaque
stringData:
  # Azure AD Application (Client) ID
  OAUTH2_PROXY_CLIENT_ID: "your-azure-ad-client-id"

  # Azure AD Application Client Secret
  OAUTH2_PROXY_CLIENT_SECRET: "your-azure-ad-client-secret"

  # Cookie secret - generate with: openssl rand -hex 16
  OAUTH2_PROXY_COOKIE_SECRET: "generate-with-openssl-rand-hex-16"
